  pod 'AWSUserPoolsSignIn', '~> 2.9.0'  # Optional dependency required to use drop-in UI
  # pod 'AWSS3', '~> 2.9.0'   		# For file transfers
  pod 'GoogleSignIn'

  target 'ProductivityTrackerTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
